extends layout.pug
append topMenu
//- append topContent
    //-event name goes here

//- Seating chart ticket prices will go here
block topContent
        ul
            each eventkey in eventinfo
                li= 'Event: ' + eventkey.eventname
                li= 'Musician: ' + eventkey.musician
                li= 'Date: ' + eventkey.date
                li= 'Venue: ' + eventkey.venue
                li= 'Location: ' + eventkey.venuecity + ', ' + eventkey.venuestate
                li= 'Genre: ' + eventkey.genre
                button(type='button' id='faveButton' value='Fave')

append bottomContent